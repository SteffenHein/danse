setup: perm ${DSC_INST}${FORMER_EXEC} ${DSC_INST}${SOLVER_EXEC} \
${DSC_INST}${POSTER_EXEC}
	@echo ">> ${UP2DT}"

all: setup

solver: perm ${DSC_INST}${SOLVER_EXEC}

former: perm ${DSC_INST}${FORMER_EXEC}

poster: perm ${DSC_INST}${POSTER_EXEC}

model: perm ${DSC_INST}${FORMER_EXEC} ${DSC_INST}${POSTER_EXEC}
	@echo ">> ${DSCOK}"

${SOLVER_EXEC}: ${DSC_DMON}
	@echo "===> S-Linking ${DSC_DMON} to ${SOLVER_EXEC}"
	-ln -fs ${DSC_DMON} ${DSC_PATH}${SOLVER_EXEC}

${DSC_DMON}: ${DSC_PATH}${DSC_DMON}
	@echo ">> ${READY}"

${FORMER_EXEC}: ${DSC_PATH}${FORMER_EXEC}
	@echo ">> ${READY}"

${POSTER_EXEC}: ${DSC_PATH}${POSTER_EXEC}
	@echo ">> ${READY}"

${DSC_INST}${SOLVER_EXEC}: ${DSC_INST}${DSC_DMON}
	@echo "===> S-Linking ${DSC_DMON} to ${SOLVER_EXEC}"
	-ln -fs ${DSC_INST}${DSC_DMON} ${DSC_INST}${SOLVER_EXEC}

${DSC_INST}${DSC_DMON}: ${DSC_PATH}${DSC_DMON}
	@echo "===> Installing ${DSC_DMON} into ${DSC_INST}"
	@if \

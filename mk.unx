${DSC_OBJ}model1.o: ${CONF} ${FORMER_SRC}FORMER.CONF ${MODEL} \
${TOOLS_SRC}panels.h \
${TOOLS_SRC}inputs.h
	@echo "===> Building DSC model"
	@if \
	${FORMER_CC} ${FORMER_CFLG} -c -D_PRE_MODEL ${MODEL} \
-o ${DSC_OBJ}model1.o; \
	then \
	echo ">> ${NICE}"; \
	fi

${DSC_OBJ}model2.o: ${CONF} ${POSTER_SRC}POSTER.CONF ${MODEL}
	@echo "===> Building DSC model evaluator"
	@if \
	${POSTER_CC} ${POSTER_CFLG} -c -D_POST_MODEL ${MODEL} \
-o ${DSC_OBJ}model2.o; \
	then \
	echo ">> ${NICE}"; \
	fi

# install all binaries [ in directories ..._INST ]:
install: setup
	@echo "===> Fixing permissions"
	-@chown -f ${DSC_USR} ${DSC_INST}
	-@chgrp -f ${DSC_GRP} ${DSC_INST}
	-@chmod -f u=rwx,g=rwx,o=rwx ${DSC_INST}${DSC_DMON}
	-@chmod -f u=rwx,g=rwx,o=rwx ${DSC_INST}${FORMER_EXEC}
	-@chmod -f u=rwx,g=rwx,o=rwx ${DSC_INST}${SOLVER_EXEC}
	-@chmod -f u=rwx,g=rwx,o=rwx ${DSC_INST}${POSTER_EXEC}

# deinstall binary programs [ in directories ..._INST ]:
deinstall: perm
	@echo "===> Deinstalling ${DISTNAME}"
	@echo "===> Cleaning objects"
	-rm -f ${DSC_OBJ}*.o
	@echo "===> Cleaning for solver"
	-rm -f ${DSC_INST}${SOLVER_EXEC}
	-rm -f ${DSC_PATH}${DSC_DMON}
	@echo "===> Cleaning for former"
	-rm -f ${DSC_INST}${FORMER_EXEC}
	-rm -f ${DSC_PATH}${FORMER_EXEC}
	@echo "===> Cleaning for poster"
	-rm -f ${DSC_INST}${POSTER_EXEC}
	-rm -f ${DSC_PATH}${POSTER_EXEC}

# clean binary programs [ in directories ..._PATH ] and all object files:
clean: perm
	@echo "===> Cleaning for ${DISTNAME}"
	@echo "===> Cleaning objects"
	-rm -f ${DSC_OBJ}*.o
	@echo "===> Cleaning for solver"
	-rm -f ${DSC_PATH}${DSC_DMON}
	-rm -f ${DSC_PATH}${SOLVER_EXEC}
	@echo "===> Cleaning for former"
	-rm -f ${DSC_PATH}${FORMER_EXEC}
	@echo "===> Cleaning for poster"
	-rm -f ${DSC_PATH}${POSTER_EXEC}
	@echo ">> ${CLEAN}"

perm:
	-@chmod -f u=rwx,g=rwx,o=rwx ${DSC_OBJ}
	-@chmod -f u=rwx,g=rwx,o=rwx ${DSC_BIN}

# reinstall binary programs [ in directories ..._INST ]:
reinstall: clean install
#-----------------------------------------------------------------------#
NICE=Looks nice ...
READY=OK, done !
DSCOK=DSC model OK !
UP2DT=Binaries up to date
CLEAN=Terminated
